                           <mif:annotations>
                              <mif:definition>
                                 <mif:text>
                                    <mif:p>
                                       <mif:p>Indicates the usage period for which the request/query applies.</mif:p>
                                    </mif:p>
                                    <mif:p>
                                       <mif:p>Filter the result set to include only those records (prescription order and prescription dispense) for which the patient was deemed to be using the device within the specified period.</mif:p>
                                    </mif:p>
                                 </mif:text>
                              </mif:definition>
                              <mif:rationale>
                                 <mif:text>
                                    <mif:p>
                                       <mif:p>Allows the requester to specify the usage period of interest for the retrieval. Useful to avoid run-away queries.</mif:p>
                                    </mif:p>
                                 </mif:text>
                              </mif:rationale>
