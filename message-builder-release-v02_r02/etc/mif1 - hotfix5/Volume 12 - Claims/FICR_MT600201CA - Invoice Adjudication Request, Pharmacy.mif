                                                                                                            conformance="R"
                                                                                                            isMandatory="true"
                                                                                                            isStructural="false">
                                                                                                <mif:businessName name="Covered Party Relationship to Policy Holder"/>
                                                                                                <mif:annotations>
                                                                                                   <mif:definition>
                                                                                                      <mif:text>
                                                                                                         <mif:p>
                                                                                                            <mif:p>The covered party relationship to the Policy Holder.</mif:p>
                                                                                                         </mif:p>
                                                                                                      </mif:text>
                                                                                                   </mif:definition>
                                                                                                   <mif:usageNotes>
                                                                                                      <mif:text>
                                                                                                         <mif:p>
                                                                                                            <mif:p>For Invoice and Pre-Determination Request &amp; all Results: Mandatory, otherwise Required.</mif:p>
                                                                                                         </mif:p>
                                                                                                      </mif:text>
                                                                                                   </mif:usageNotes>
                                                                                                   <mif:rationale>
                                                                                                      <mif:text>
                                                                                                         <mif:p>
                                                                                                            <mif:p>Reason for being covered, including spouse, child, students, handicapped</mif:p>
