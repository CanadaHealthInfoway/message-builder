                                                                                             <mif:annotations>
                                                                                                <mif:definition>
                                                                                                   <mif:text>
                                                                                                      <mif:p>
                                                                                                         <mif:p>Covered Person Identifier</mif:p>
                                                                                                      </mif:p>
                                                                                                      <mif:p>
                                                                                                         <mif:p>(Assigned by carrier - root of OID)</mif:p>
                                                                                                      </mif:p>
                                                                                                      <mif:p>
                                                                                                         <mif:p>= Subscriber+Dependents</mif:p>
                                                                                                      </mif:p>
                                                                                                      <mif:p>
                                                                                                         <mif:p>Can be a single covered party,</mif:p>
                                                                                                      </mif:p>
                                                                                                      <mif:p>
                                                                                                         <mif:p>an organization, an animal</mif:p>
                                                                                                      </mif:p>
                                                                                                      <mif:p>
                                                                                                         <mif:p>or a group of patients</mif:p>
                                                                                                      </mif:p>
                                                                                                      <mif:p>
                                                                                                         <mif:p>and/or a group of animals.</mif:p>
                                                                                                      </mif:p>
                                                                                                      <mif:p>
                                                                                                         <mif:p>Covered party may be a patient who for the purposes of the invoice is a covered party.</mif:p>
                                                                                                      </mif:p>
                                                                                                   </mif:text>
                                                                                                   <mif:graphicRepresentation>
                                                                                                      <mif:graphElement shapeId="Sheet.57" containerDiagramName="Main" isWidthAutoSize="true"
                                                                                                                        isHeightAutoSize="true">
