                           <mif:type name="CS"/>
                           <mif:supplierDomainSpecification codingStrength="CNE" codeSystemName="ActRelationshipType" mnemonic="RSON"/>
                        </mif:attribute>
                        <mif:association sortKey="34">
                           <mif:graphicRepresentation>
