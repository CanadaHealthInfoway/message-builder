                  <mif:participantClass>
                     <mif:class name="PaymentRequestReason" isAbstract="false">
                        <mif:businessName name="Payment Request for"/>
                        <mif:derivationSupplier staticModelDerivationId="1" className="ActRelationship"/>
                        <mif:graphicRepresentation presentation="HL7">
                           <mif:graphElement shapeId="Sheet.111" containerDiagramName="Main" shapeTemplate="ActRelationship"
                                             textWrappingWidth="66.0"
                                             nodeOrientation="Up">
                              <mif:position x="11.825" y="6.832"/>
                              <mif:size width="1.190" height="0.696"/>
                           </mif:graphElement>
                        </mif:graphicRepresentation>
                        <mif:annotations>
                           <mif:definition>
                              <mif:text>
                                 <mif:p>
                                    <mif:p>The reason or rationale for the invoice.</mif:p>
                                 </mif:p>
                              </mif:text>
                           </mif:definition>
                           <mif:usageNotes>
                              <mif:text>
                                 <mif:p>
                                    <mif:p>For Pharmacy dispense Invoices (RXDINV, RXCINV), only 1 Invoice Grouping is permitted to facilitate real-time processing.</mif:p>
                                 </mif:p>
                                 <mif:p>
                                    <mif:p>For Pharmacy clinical product (CPINV) and clinical services (CSINV) Invoices, 1 or more Invoice Groupings are permitted.</mif:p>
                                 </mif:p>
                                 <mif:p>
                                    <mif:p>For Coverage Extension Requests, only 1 Invoice Grouping per Payment Request.</mif:p>
                                 </mif:p>
                                 <mif:p>
                                    <mif:p>RxS1: Only allow 1 Invoice Grouping. Maximum cardinality is 1.</mif:p>
                                 </mif:p>
                              </mif:text>
                           </mif:usageNotes>
                           <mif:rationale>
                              <mif:text>
                                 <mif:p>
                                    <mif:p>For Cancel Request: To cancel an Invoice with more than 1 Invoice Grouping, each individual Invoice Group must be cancelled separately using an individual Cancel Invoice Request message.</mif:p>
                                 </mif:p>
                                 <mif:p>
                                    <mif:p>For Cancel Results: Must include a Payment Request and Invoice Element Group.</mif:p>
                                 </mif:p>
                              </mif:text>
                           </mif:rationale>
                        </mif:annotations>
                        <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                                       conformance="R"
                                       isMandatory="true"
                                       fixedValue="RSON"
