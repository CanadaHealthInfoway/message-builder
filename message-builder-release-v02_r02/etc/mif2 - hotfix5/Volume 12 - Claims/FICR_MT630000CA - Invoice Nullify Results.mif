         <mif:businessName name="Organisation/Individual receiving payment for claim."><realmNamespace value="UV"/></mif:businessName>
         <derivedFrom className="ActRelationship" staticModelDerivationId="1"/>
         
         <mif:annotations>
            
            
         <documentation><mif:definition>
               <mif:text>
                  <xhtml:p>
                     <xhtml:p>Organisation/Individual receiving payment for claim.</xhtml:p>
                  </xhtml:p>
               </mif:text>
