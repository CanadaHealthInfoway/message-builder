      <mif:class isAbstract="false" name="ParentOrGuardianRole">
         <mif:businessName name="Parent or Guardian of patient"><realmNamespace value="UV"/></mif:businessName>
         <derivedFrom className="Role" staticModelDerivationId="1"/>
         
