            <mif:annotations>
               
               
               
            <documentation><mif:definition>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>An indication of sensitivity surrounding the related measurable observation, and thus defines the required sensitivity for the detected issue.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:definition><mif:rationale>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>Conveys the patients wishes relating to the sensitivity of the observation information.</xhtml:p>
                     </xhtml:p>
                     <xhtml:p>
                        <xhtml:p>The attribute is optional because not all systems will support masking.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:rationale></documentation><appInfo><mif:mapping sourceName="PIN">
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>Contraindication.intractingSourceMasked (Normal=false; Restricted or Very Restricted = True); (Information is withheld because the prescription is masked will be given a NULL flavour of 'Masked')</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:mapping></appInfo></mif:annotations>
            <derivedFrom attributeName="confidentialityCode" className="Observation" staticModelDerivationId="1"/>
            <mif:type name="CV"/>
            <supplierVocabulary codingStrength="CNE"><vocabularyDomain name="x_VeryBasicConfidentialityKind"/></supplierVocabulary>
         </mif:attribute>
         <mif:attribute conformance="R" isImmutable="false" isMandatory="false" maximumMultiplicity="1" minimumMultiplicity="0" name="value" sortKey="7">
            <mif:businessName name="C:Observation Value"><realmNamespace value="UV"/></mif:businessName>
            <mif:annotations>
               
               
            <documentation><mif:definition>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>Denotes a specific measurable observation made about a person that might have trigger the clinical issue detection.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:definition><mif:rationale>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>Provides unambiguous reference to the related measurable observation.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:rationale></documentation></mif:annotations>
            <derivedFrom attributeName="value" className="Observation" staticModelDerivationId="1"/>
            <mif:type name="PQ.BASIC"/>
         </mif:attribute>
      </mif:class>
