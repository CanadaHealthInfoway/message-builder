                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>C7</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:mapping></appInfo></mif:annotations>
            <derivedFrom attributeName="expirationTime" className="ManufacturedMaterial" staticModelDerivationId="1"/>
            <mif:type name="IVL.HIGH">
