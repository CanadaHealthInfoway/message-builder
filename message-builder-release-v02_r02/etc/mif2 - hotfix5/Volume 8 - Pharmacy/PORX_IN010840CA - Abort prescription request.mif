<?xml version="1.0" encoding="UTF-8"?>
<mif:interaction xmlns:mif="urn:hl7-org:v3/mif" schemaVersion="2.0" name="010840"
                 title="Common Order Order Abort Request">
   <mif:businessName name="Abort prescription request">
