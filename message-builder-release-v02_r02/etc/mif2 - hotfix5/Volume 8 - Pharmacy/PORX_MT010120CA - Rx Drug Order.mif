            <mif:annotations>
               
               
            <documentation><mif:definition>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>The date and time on which the dispense is expected to be picked up.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:definition><mif:rationale>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>Allows a prescriber to indicate to the targeted pharmacy, when patient will be expecting to pick up the dispensed medication.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:rationale></documentation></mif:annotations>
            <derivedFrom attributeName="time" className="Participation" staticModelDerivationId="1"/>
            <mif:type name="IVL.LOW">
               <argumentDatatype name="TS.FULLDATE"/>
            </mif:type>
         </mif:attribute>
         <mif:attribute conformance="R" isImmutable="false" isMandatory="false" maximumMultiplicity="1" minimumMultiplicity="0" name="substitutionConditionCode" sortKey="4">
            <mif:businessName name="Dispense Facility Not Reassignable"><realmNamespace value="UV"/></mif:businessName>
            <mif:annotations>
               
               
            <documentation><mif:definition>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>Indicates a 'hard' or 'soft' assignment of dispensing priviledged to the targetted facility.</xhtml:p>
                     </xhtml:p>
                     <xhtml:p>
                        <xhtml:p>'Hard' assignment (mandated facility) indicates that the prescription can be dispensed only at that facility.</xhtml:p>
                     </xhtml:p>
                     <xhtml:p>
                        <xhtml:p>'Soft' assignment (usually as a patient directive) indicates that the prescription may be dispensed at facilities other than the targeted facility.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:definition><mif:rationale>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>Influences whether the prescription may be transferred to a service delivery location other than the targeted dispenser.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:rationale></documentation></mif:annotations>
            <derivedFrom attributeName="substitutionConditionCode" className="Participation" staticModelDerivationId="1"/>
            <mif:type name="CV"/>
            <supplierVocabulary codingStrength="CNE"><vocabularyDomain name="x_SubstitutionConditionNoneOrUnconditional"/></supplierVocabulary>
         </mif:attribute>
