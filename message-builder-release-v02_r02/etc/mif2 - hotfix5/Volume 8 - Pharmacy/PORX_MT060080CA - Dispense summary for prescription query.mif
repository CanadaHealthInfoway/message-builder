            <mif:businessName name="D:Prescription Order Number"><realmNamespace value="UV"/></mif:businessName>
            <mif:annotations>
               
               
            <documentation><mif:definition>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>Identifier of the prescription for which detailed information is required.</xhtml:p>
                     </xhtml:p>
                     <xhtml:p>
                        <xhtml:p>The result set will be filtered to only the specific prescription.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:definition><mif:rationale>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>Identifies the prescription that is to be retrieved and is therefore mandatory.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:rationale></documentation></mif:annotations>
            <derivedFrom attributeName="value" className="ParameterItem" staticModelDerivationId="1"/>
            <mif:type name="II.BUSVER"/>
         </mif:attribute>
      </mif:class>
