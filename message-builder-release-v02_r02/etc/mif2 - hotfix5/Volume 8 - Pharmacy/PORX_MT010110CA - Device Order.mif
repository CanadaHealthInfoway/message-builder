   <containedClass>
      <mif:class isAbstract="false" name="Precondition">
         <mif:businessName name="Non-authoritative?"><realmNamespace value="UV"/></mif:businessName>
         <derivedFrom className="ActRelationship" staticModelDerivationId="1"/>
         
         <mif:annotations>
            
            
         <documentation><mif:definition>
               <mif:text>
                  <xhtml:p>
                     <xhtml:p>If present, indicates that the prescription is non-authoritative. I.e. A paper copy must be viewed before the prescription can be dispensed.</xhtml:p>
                  </xhtml:p>
               </mif:text>
            </mif:definition><mif:rationale>
               <mif:text>
                  <xhtml:p>
                     <xhtml:p>Allows distinguishing prescriptions where the paper copy is authoritative vs. the electronic copy being authoritative.</xhtml:p>
                  </xhtml:p>
                  <xhtml:p>
                     <xhtml:p>This attribute behaves as a 'mandatory' element where the presence of the association indicates that a paper prescription is needed and the absence of the association indicates that one is not.</xhtml:p>
                  </xhtml:p>
               </mif:text>
            </mif:rationale></documentation></mif:annotations>
         <mif:attribute conformance="R" isImmutable="true" isMandatory="true" maximumMultiplicity="1" minimumMultiplicity="1" name="typeCode" sortKey="1"><enumerationValues>PRCN</enumerationValues>
            <derivedFrom attributeName="typeCode" className="ActRelationship" staticModelDerivationId="1"/>
            <mif:type name="CS"/>
            <supplierVocabulary codingStrength="CNE"><code code="PRCN" codeSystemName="ActRelationshipType"/></supplierVocabulary>
         </mif:attribute>
