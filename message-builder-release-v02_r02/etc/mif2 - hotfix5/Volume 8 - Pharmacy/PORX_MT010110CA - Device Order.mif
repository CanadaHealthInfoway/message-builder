         <mif:annotations>
            
            
         <documentation><mif:definition>
               <mif:text>
                  <xhtml:p>
                     <xhtml:p>Denotes the reason(s) for this specific prescription; it must not be interpreted as a permanent diagnosis.</xhtml:p>
                  </xhtml:p>
               </mif:text>
            </mif:definition><mif:rationale>
               <mif:text>
                  <xhtml:p>
                     <xhtml:p>Helps providers evaluate the appropriateness of the instructions for the device, and may influence education or literature provided to the patient on the use of the device.</xhtml:p>
                  </xhtml:p>
                  <xhtml:p>
                     <xhtml:p>Provided at the discretion of the prescriber to enhance patient care.</xhtml:p>
                  </xhtml:p>
                  <xhtml:p>
                     <xhtml:p>This field is marked as populated because of its high clinical importance, however 'Nulls' are allowed because the individual recording the prescription (e.g. pharmacist) may not be aware of the indication or the prescriber may choose to withhold (mask) the information due to patient sensitivity.</xhtml:p>
                  </xhtml:p>
               </mif:text>
            </mif:rationale></documentation></mif:annotations>
         <mif:attribute conformance="R" isImmutable="true" isMandatory="true" maximumMultiplicity="1" minimumMultiplicity="1" name="typeCode" sortKey="1"><enumerationValues>RSON</enumerationValues>
            <derivedFrom attributeName="typeCode" className="ActRelationship" staticModelDerivationId="1"/>
            <mif:type name="CS"/>
            <supplierVocabulary codingStrength="CNE"><code code="RSON" codeSystemName="ActRelationshipType"/></supplierVocabulary>
