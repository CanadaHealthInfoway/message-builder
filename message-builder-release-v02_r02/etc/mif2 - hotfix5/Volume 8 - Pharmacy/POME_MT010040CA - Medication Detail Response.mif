   <containedClass>
      <mif:class isAbstract="false" name="Document">
         <mif:businessName name="Monographs"><realmNamespace value="UV"/></mif:businessName>
         <derivedFrom className="Document" staticModelDerivationId="1"/>
         
         <mif:annotations>
            
            
         <documentation><mif:definition>
               <mif:text>
                  <xhtml:p>
                     <xhtml:p>Information on the composition and use of a drug.</xhtml:p>
                  </xhtml:p>
               </mif:text>
            </mif:definition><mif:rationale>
               <mif:text>
