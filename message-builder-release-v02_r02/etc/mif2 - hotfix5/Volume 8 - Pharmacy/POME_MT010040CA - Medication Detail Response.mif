            <derivedFrom attributeName="moodCode" className="Observation" staticModelDerivationId="1"/>
            <mif:type name="CS"/>
            <supplierVocabulary codingStrength="CNE"><code code="EVN.CRT" codeSystemName="ActMood"/></supplierVocabulary>
         </mif:attribute>
         <mif:attribute conformance="R" isImmutable="false" isMandatory="true" maximumMultiplicity="1" minimumMultiplicity="1" name="code" sortKey="3">
            <mif:businessName name="Patient Characteristic Type"><realmNamespace value="UV"/></mif:businessName>
            <mif:annotations>
               
               
            <documentation><mif:definition>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>Indicates the type of patient characteristic being expressed. E.g. Height, weight, age, lab values, etc. If negation indicator is true, then this indicates a characteristic the patient should *not* have.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:definition><mif:rationale>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>Needed to identify what type of characteristic is being expressed, and therefore mandatory.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:rationale></documentation></mif:annotations>
            <derivedFrom attributeName="code" className="Observation" staticModelDerivationId="1"/>
            <mif:type name="CV"/>
            <supplierVocabulary codingStrength="CNE"><vocabularyDomain name="ObservationDosageDefinitionPreconditionType"/></supplierVocabulary>
         </mif:attribute>
         <mif:attribute conformance="R" defaultValue="false" isImmutable="true" isMandatory="true" maximumMultiplicity="1" minimumMultiplicity="1" name="negationInd" sortKey="4">
            <mif:businessName name="Exclude characteristic?"><realmNamespace value="UV"/></mif:businessName>
            <mif:annotations>
               
               
            <documentation><mif:definition>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>If true, indicates that the characteristic is one which should *not* be held by the patient for the dosage to apply.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:definition><mif:rationale>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>Many dosages are inappropriate for patients with certain characteristics (e.g. INR values, etc.)</xhtml:p>
                     </xhtml:p>
                     <xhtml:p>
                        <xhtml:p>Because it must be known whether the characteristic is included or excluded, this element is mandatory.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:rationale></documentation></mif:annotations>
            <derivedFrom attributeName="negationInd" className="Observation" staticModelDerivationId="1"/>
            <mif:type name="BL"/>
         </mif:attribute>
         <mif:attribute conformance="R" isImmutable="false" isMandatory="false" maximumMultiplicity="1" minimumMultiplicity="1" name="value" sortKey="5">
            <mif:businessName name="Patient Characteristic Value"><realmNamespace value="UV"/></mif:businessName>
            <mif:annotations>
               
               
            <documentation><mif:definition>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>Indicates the specific value or range of values of the characteristic a patient should have for the dosage to be appropriate.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:definition><mif:rationale>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>Allows comparison to actual patient characteristics to see if the dosage is applicable. In some circumstances, the specific range may not be known, thus the field is "populated"</xhtml:p>
                     </xhtml:p>
                     <xhtml:p>
                        <xhtml:p>Example: This dosage specification applies to people over 60 pounds".</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:rationale></documentation></mif:annotations>
            <derivedFrom attributeName="value" className="Observation" staticModelDerivationId="1"/>
            <mif:type name="IVL">
               <argumentDatatype name="PQ.BASIC"/>
            </mif:type>
         </mif:attribute>
      </mif:class>
