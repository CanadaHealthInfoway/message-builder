   <entryPoint className="ParameterList" id="PORX_MT050010CA" isAbstract="false" name="Drug contraindication query">
      <mif:annotations>
         
      <documentation><mif:description>
            <mif:text>
               <xhtml:p>Entry point for patient drug contraindication query message</xhtml:p>
            </mif:text>
         </mif:description></documentation></mif:annotations>
      
   </entryPoint>
   <containedClass>
      <mif:class isAbstract="false" name="DrugCode">
         <derivedFrom className="ParameterItem" staticModelDerivationId="1"/>
         
         <mif:attribute conformance="R" isImmutable="false" isMandatory="true" maximumMultiplicity="1" minimumMultiplicity="1" name="value" sortKey="1">
            <mif:businessName name="Drug Codes"><realmNamespace value="UV"/></mif:businessName>
            <mif:annotations>
               
               
            <documentation><mif:definition>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>Indicates that the result set is to be filtered to include only those contraindications involving the specified drugs.</xhtml:p>
                     </xhtml:p>
                     <xhtml:p>
                        <xhtml:p>The codes may refer to administerable medications, orderable medications or higher level drug classifications.</xhtml:p>
                     </xhtml:p>
                     <xhtml:p>
                        <xhtml:p>The set of contraindication records retrieved would include all potential DDI between and amongst the drugs.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:definition><mif:rationale>
                  <mif:text>
                     <xhtml:p>
                        <xhtml:p>Allows for the retrieval of all potential contraindications that would result from drug profile scenarios.</xhtml:p>
                     </xhtml:p>
                     <xhtml:p>
                        <xhtml:p>This will most commonly be used by providers to test the viability of prescribing/dispensing medications into profile scenarios.</xhtml:p>
                     </xhtml:p>
                  </mif:text>
               </mif:rationale></documentation></mif:annotations>
            <derivedFrom attributeName="value" className="ParameterItem" staticModelDerivationId="1"/>
            <mif:type name="CV"/>
            <supplierVocabulary codingStrength="CNE"><vocabularyDomain name="ClinicalDrug"/></supplierVocabulary>
         </mif:attribute>
      </mif:class>
   </containedClass>
   <containedClass>
