   <containedClass>
      <mif:class isAbstract="false" name="Patient">
         <mif:businessName name="Patient"><realmNamespace value="UV"/></mif:businessName>
         <derivedFrom className="Patient" staticModelDerivationId="1"/>
         
         <mif:annotations>
            
            
         <documentation><mif:definition>
               <mif:text>
                  <xhtml:p>
                     <xhtml:p>Indicates that the keyword was changed by the patient themselves.</xhtml:p>
                  </xhtml:p>
               </mif:text>
            </mif:definition><mif:rationale>
               <mif:text>
                  <xhtml:p>
                     <xhtml:p>Sometimes keywords are changed by the patient, sometimes by a patient representative.</xhtml:p>
                  </xhtml:p>
               </mif:text>
            </mif:rationale></documentation></mif:annotations>
         <mif:attribute conformance="R" isImmutable="true" isMandatory="true" maximumMultiplicity="1" minimumMultiplicity="1" name="classCode" sortKey="1"><enumerationValues>PAT</enumerationValues>
