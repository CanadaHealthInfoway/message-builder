                                                                                                                        </mif:graphElement>
                                                                                                                     </mif:graphicRepresentation>
                                                                                                                     <mif:annotations>
                                                                                                                        <mif:definition>
                                                                                                                           <mif:text>
                                                                                                                              <mif:p>
                                                                                                                                 <mif:p>
                                                                                                                                    <mif:i>A particular request (referral or specific request) that resulted in the creation of the Discharge-Care Summary.</mif:i>
                                                                                                                                 </mif:p>
                                                                                                                              </mif:p>
                                                                                                                           </mif:text>
                                                                                                                        </mif:definition>
                                                                                                                        <mif:rationale>
                                                                                                                           <mif:text>
                                                                                                                              <mif:p>
                                                                                                                                 <mif:p>
                                                                                                                                    <mif:i>There are several pieces of information about a request that may influence understanding of the record including when requested, by whom, why, etc.</mif:i>
                                                                                                                                 </mif:p>
                                                                                                                              </mif:p>
                                                                                                                           </mif:text>
                                                                                                                        </mif:rationale>
                                                                                                                     </mif:annotations>
                                                                                                                     <mif:attribute name="classCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                                                                                                                                    conformance="R"
                                                                                                                                    isMandatory="true"
                                                                                                                                    isStructural="true">
                                                                                                                        <mif:businessName name="B: Request Type"/>
                                                                                                                        <mif:annotations>
                                                                                                                           <mif:definition>
                                                                                                                              <mif:text>
                                                                                                                                 <mif:p>
                                                                                                                                    <mif:p>
                                                                                                                                       <mif:i>This distinguishes between "referral" requests and specific orders.</mif:i>
                                                                                                                                    </mif:p>
                                                                                                                                 </mif:p>
                                                                                                                              </mif:text>
                                                                                                                           </mif:definition>
                                                                                                                           <mif:rationale>
                                                                                                                              <mif:text>
                                                                                                                                 <mif:p>
                                                                                                                                    <mif:p>
                                                                                                                                       <mif:i>Needed to know what query to execute to retrieve details about the request. (I.e. Get Patient Referral vs. Get Discharge-Care Summary Order.)</mif:i>
                                                                                                                                    </mif:p>
                                                                                                                                 </mif:p>
                                                                                                                              </mif:text>
                                                                                                                           </mif:rationale>
                                                                                                                        </mif:annotations>
                                                                                                                        <mif:derivationSupplier staticModelDerivationId="1" className="Act" attributeName="classCode"/>
                                                                                                                        <mif:type name="CS"/>
                                                                                                                        <mif:supplierDomainSpecification codingStrength="CNE" valueSetName="x_ActClassCareProvisionEncounter"/>
                                                                                                                     </mif:attribute>
