                                                                                          <mif:definition>
                                                                                             <mif:text>
                                                                                                <mif:p>
                                                                                                   <mif:p>Represents the label associated with this particular portion of the document. These are a human-readable equivalents to the Section Type code. E.g. "Allergies", "Assessment", "Recommendations", etc.</mif:p>
                                                                                                </mif:p>
                                                                                             </mif:text>
                                                                                          </mif:definition>
                                                                                          <mif:rationale>
                                                                                             <mif:text>
                                                                                                <mif:p>
                                                                                                   <mif:p>Provides a human readable label for the section. Because it is used as part of the document rendering, the attribute is mandatory.</mif:p>
                                                                                                </mif:p>
                                                                                             </mif:text>
                                                                                          </mif:rationale>
                                                                                       </mif:annotations>
