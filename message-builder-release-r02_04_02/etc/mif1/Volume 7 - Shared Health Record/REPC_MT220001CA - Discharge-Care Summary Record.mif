                                                                                                                                    isMandatory="false"
                                                                                                                                    isStructural="false">
                                                                                                                        <mif:businessName name="B:Diagnosis Code"/>
                                                                                                                        <mif:annotations>
                                                                                                                           <mif:definition>
                                                                                                                              <mif:text>
                                                                                                                                 <mif:p>
                                                                                                                                    <mif:p>Identifies the actual discharge diagnosis.</mif:p>
                                                                                                                                 </mif:p>
                                                                                                                              </mif:text>
                                                                                                                           </mif:definition>
                                                                                                                           <mif:rationale>
                                                                                                                              <mif:text>
                                                                                                                                 <mif:p>
                                                                                                                                    <mif:p>Provides context into the result of the care provision.</mif:p>
                                                                                                                                 </mif:p>
                                                                                                                                 <mif:p>
                                                                                                                                    <mif:p>
                                                                                                                                       <mif:i>The attribute is optional because for systems that support SNOMED, this concept will be communicated as part of the Discharge Diagnosis Type</mif:i>
                                                                                                                                    </mif:p>
                                                                                                                                 </mif:p>
                                                                                                                              </mif:text>
                                                                                                                           </mif:rationale>
                                                                                                                        </mif:annotations>
