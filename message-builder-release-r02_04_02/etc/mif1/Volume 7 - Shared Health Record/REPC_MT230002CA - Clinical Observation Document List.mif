                                                                                                                     <mif:businessName name="Request"/>
                                                                                                                     <mif:derivationSupplier staticModelDerivationId="1" className="Act"/>
                                                                                                                     <mif:graphicRepresentation presentation="HL7">
                                                                                                                        <mif:graphElement shapeId="Sheet.968" containerDiagramName="Main" shapeTemplate="Act"
                                                                                                                                          isWidthAutoSize="true"
                                                                                                                                          isHeightAutoSize="true">
                                                                                                                           <mif:position x="22.890" y="1.542"/>
                                                                                                                           <mif:size width="2.862" height="0.781"/>
                                                                                                                        </mif:graphElement>
                                                                                                                     </mif:graphicRepresentation>
                                                                                                                     <mif:annotations>
                                                                                                                        <mif:definition>
                                                                                                                           <mif:text>
                                                                                                                              <mif:p>
                                                                                                                                 <mif:p>
                                                                                                                                    <mif:i>A particular request (referral or specific request) that resulted in the creation of the Clinical Observation Document.</mif:i>
                                                                                                                                 </mif:p>
                                                                                                                              </mif:p>
                                                                                                                           </mif:text>
                                                                                                                        </mif:definition>
                                                                                                                        <mif:rationale>
                                                                                                                           <mif:text>
                                                                                                                              <mif:p>
                                                                                                                                 <mif:p>
                                                                                                                                    <mif:i>There are several pieces of information about a request that may influence understanding of the record including when requested, by whom, why, etc.</mif:i>
                                                                                                                                 </mif:p>
                                                                                                                              </mif:p>
                                                                                                                           </mif:text>
                                                                                                                        </mif:rationale>
                                                                                                                     </mif:annotations>
                                                                                                                     <mif:attribute name="classCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                                                                                                                                    conformance="R"
                                                                                                                                    isMandatory="true"
                                                                                                                                    isStructural="true">
                                                                                                                        <mif:businessName name="B: Request Type"/>
                                                                                                                        <mif:annotations>
                                                                                                                           <mif:definition>
                                                                                                                              <mif:text>
                                                                                                                                 <mif:p>
                                                                                                                                    <mif:p>
                                                                                                                                       <mif:i>This distinguishes between "referral" requests and specific orders.</mif:i>
                                                                                                                                    </mif:p>
                                                                                                                                 </mif:p>
                                                                                                                              </mif:text>
                                                                                                                           </mif:definition>
                                                                                                                           <mif:rationale>
                                                                                                                              <mif:text>
                                                                                                                                 <mif:p>
                                                                                                                                    <mif:p>
                                                                                                                                       <mif:i>Needed to know what query to execute to retrieve details about the request. (I.e. Get Patient Referral vs. Get Clinical Observation Document Order.)</mif:i>
                                                                                                                                    </mif:p>
                                                                                                                                 </mif:p>
                                                                                                                              </mif:text>
                                                                                                                           </mif:rationale>
                                                                                                                        </mif:annotations>
                                                                                                                        <mif:derivationSupplier staticModelDerivationId="1" className="Act" attributeName="classCode"/>
                                                                                                                        <mif:type name="CS"/>
                                                                                                                        <mif:supplierDomainSpecification codingStrength="CNE" valueSetName="x_ActClassCareProvisionObservation"/>
                                                                                                                     </mif:attribute>
                                                                                                                     <mif:attribute name="moodCode" sortKey="2" minimumMultiplicity="1" maximumMultiplicity="1"
                                                                                                                                    conformance="R"
                                                                                                                                    isMandatory="true"
                                                                                                                                    fixedValue="RQO"
                                                                                                                                    isStructural="true">
                                                                                                                        <mif:derivationSupplier staticModelDerivationId="1" className="Act" attributeName="moodCode"/>
                                                                                                                        <mif:type name="CS"/>
                                                                                                                        <mif:supplierDomainSpecification codingStrength="CNE" codeSystemName="ActMood" mnemonic="RQO"/>
                                                                                                                     </mif:attribute>
                                                                                                                     <mif:attribute name="id" sortKey="3" minimumMultiplicity="0" maximumMultiplicity="1"
                                                                                                                                    conformance="R"
                                                                                                                                    isMandatory="false"
                                                                                                                                    isStructural="false">
                                                                                                                        <mif:businessName name="A: Request Id"/>
                                                                                                                        <mif:annotations>
                                                                                                                           <mif:definition>
                                                                                                                              <mif:text>
                                                                                                                                 <mif:p>
                                                                                                                                    <mif:p>
                                                                                                                                       <mif:i>This identifies the specific request record (referral or order) that this Clinical Observation Document is fulfilling. The type of request is identified by the Request Type.</mif:i>
                                                                                                                                    </mif:p>
                                                                                                                                 </mif:p>
                                                                                                                              </mif:text>
                                                                                                                           </mif:definition>
                                                                                                                           <mif:rationale>
                                                                                                                              <mif:text>
                                                                                                                                 <mif:p>
                                                                                                                                    <mif:p>
                                                                                                                                       <mif:i>Associates a request to its results. Allows querying for results associated by a request. Also, authors of requests are generally authorized to view associated results, even if masked. Result records may also be automatically retrieved when retrieving the request record.</mif:i>
                                                                                                                                    </mif:p>
                                                                                                                                    <mif:p>
                                                                                                                                       <mif:i>This attributes is only "required" because in some cases there might not be a formal electronic request. However, the requesting provider may still be known.</mif:i>
                                                                                                                                    </mif:p>
                                                                                                                                 </mif:p>
                                                                                                                              </mif:text>
                                                                                                                           </mif:rationale>
                                                                                                                        </mif:annotations>
                                                                                                                        <mif:derivationSupplier staticModelDerivationId="1" className="Act" attributeName="id"/>
                                                                                                                        <mif:type name="II.BUS"/>
                                                                                                                     </mif:attribute>
                                                                                                                     <mif:association sortKey="33">
                                                                                                                        <mif:graphicRepresentation>
                                                                                                                           <mif:graphElement connectToShapeId="Sheet.968" containerDiagramName="Main">
                                                                                                                              <mif:position x="25.752" y="1.932"/>
                                                                                                                              <mif:graphEdge>
                                                                                                                                 <mif:anchor connectToShapeId="Sheet.972">
                                                                                                                                    <mif:position x="25.752" y="1.932"/>
                                                                                                                                 </mif:anchor>
                                                                                                                              </mif:graphEdge>
                                                                                                                           </mif:graphElement>
                                                                                                                        </mif:graphicRepresentation>
                                                                                                                        <mif:targetConnection name="author" minimumMultiplicity="0" maximumMultiplicity="1" conformance="R"
                                                                                                                                              isMandatory="false"
                                                                                                                                              sortKey="AEA_______">
                                                                                                                           <mif:derivationSupplier staticModelDerivationId="1" className="Act" associationEndName="participation"/>
                                                                                                                           <mif:participantClass>
                                                                                                                              <mif:class name="Author2" isAbstract="false">
                                                                                                                                 <mif:businessName name="*a: requested by"/>
                                                                                                                                 <mif:derivationSupplier staticModelDerivationId="1" className="Participation"/>
                                                                                                                                 <mif:graphicRepresentation presentation="HL7">
                                                                                                                                    <mif:graphElement shapeId="Sheet.972" containerDiagramName="Main" shapeTemplate="Participation"
                                                                                                                                                      textWrappingWidth="66.0"
                                                                                                                                                      nodeOrientation="Right">
                                                                                                                                       <mif:position x="25.752" y="1.684"/>
                                                                                                                                       <mif:size width="2.409" height="0.436"/>
                                                                                                                                    </mif:graphElement>
                                                                                                                                 </mif:graphicRepresentation>
                                                                                                                                 <mif:annotations>
                                                                                                                                    <mif:definition>
                                                                                                                                       <mif:text>
                                                                                                                                          <mif:p>
                                                                                                                                             <mif:p>
                                                                                                                                                <mif:i>This identifies the provider who requested the Clinical Observation Document to be performed. The request may have been verbal, electronic, as part of a referral or by other means. This element does not need to be captured in "record" and "amend" messages where a request id is captured, as the requesting provider can be looked up by retrieving the request.</mif:i>
                                                                                                                                             </mif:p>
                                                                                                                                          </mif:p>
                                                                                                                                       </mif:text>
                                                                                                                                    </mif:definition>
                                                                                                                                    <mif:rationale>
                                                                                                                                       <mif:text>
                                                                                                                                          <mif:p>
                                                                                                                                             <mif:p>
                                                                                                                                                <mif:i>By requesting the action, the provider takes a level of responsibility for it. The attribute may also be used to search or filter records. Requesting providers generally have authorization to view masked information as well.</mif:i>
                                                                                                                                             </mif:p>
                                                                                                                                          </mif:p>
                                                                                                                                       </mif:text>
                                                                                                                                    </mif:rationale>
                                                                                                                                 </mif:annotations>
                                                                                                                                 <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                                                                                                                                                conformance="R"
                                                                                                                                                isMandatory="true"
                                                                                                                                                fixedValue="AUT"
                                                                                                                                                isStructural="true">
                                                                                                                                    <mif:derivationSupplier staticModelDerivationId="1" className="Participation" attributeName="typeCode"/>
                                                                                                                                    <mif:type name="CS"/>
