                                                                                                                        maximumMultiplicity="1"
                                                                                                                        conformance="R"
                                                                                                                        isMandatory="true"
                                                                                                                        fixedValue="true"
                                                                                                                        isStructural="true">
                                                                                                            <mif:derivationSupplier staticModelDerivationId="1" className="ActRelationship"
                                                                                                                                    attributeName="contextConductionInd"/>
                                                                                                            <mif:type name="BL"/>
                                                                                                         </mif:attribute>
