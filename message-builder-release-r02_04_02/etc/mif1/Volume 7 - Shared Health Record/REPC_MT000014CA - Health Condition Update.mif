                        </mif:association>
                     </mif:class>
                  </mif:participantClass>
               </mif:targetConnection>
               <mif:sourceConnection>
                  <mif:nonTraversableConnection participantClassName="ConditionEvent">
                     <mif:derivationSupplier staticModelDerivationId="1" className="Participation" associationEndName="act"/>
                  </mif:nonTraversableConnection>
               </mif:sourceConnection>
            </mif:association>
            <mif:association sortKey="0">
               <mif:graphicRepresentation>
                  <mif:graphElement connectToShapeId="Sheet.1" containerDiagramName="Main">
                     <mif:position x="8.792" y="7.561"/>
                     <mif:graphEdge>
                        <mif:anchor connectToShapeId="Sheet.259">
                           <mif:position x="8.792" y="7.561"/>
                        </mif:anchor>
                     </mif:graphEdge>
                  </mif:graphElement>
               </mif:graphicRepresentation>
               <mif:targetConnection name="componentOf" minimumMultiplicity="0" maximumMultiplicity="100"
                                     conformance="R"
                                     isMandatory="false"
                                     sortKey="CE________">
                  <mif:derivationSupplier staticModelDerivationId="1" className="Act"
                                          associationEndName="inboundRelationship"/>
                  <mif:participantClass>
                     <mif:class name="Component" isAbstract="false">
                        <mif:businessName name="f:is part of"/>
                        <mif:derivationSupplier staticModelDerivationId="1" className="ActRelationship"/>
                        <mif:graphicRepresentation presentation="HL7">
                           <mif:graphElement shapeId="Sheet.259" containerDiagramName="Main" shapeTemplate="ActRelationship"
                                             textWrappingWidth="66.0"
                                             nodeOrientation="Right">
                              <mif:position x="6.067" y="7.553"/>
                              <mif:size width="2.725" height="0.569"/>
                           </mif:graphElement>
                        </mif:graphicRepresentation>
                        <mif:annotations>
                           <mif:definition>
                              <mif:text>
                                 <mif:p>
                                    <mif:p>
                                       <mif:i>This identifies the encounter-based, condition-based and/or other care-based collections that the Health Condition is considered to be part of.</mif:i>
                                    </mif:p>
                                 </mif:p>
                              </mif:text>
                           </mif:definition>
                           <mif:rationale>
                              <mif:text>
                                 <mif:p>
                                    <mif:p>
                                       <mif:i>Care compositions provide a means of navigating a patient's EHR by grouping related records together into identified "collections".</mif:i>
                                    </mif:p>
                                    <mif:p>
                                       <mif:i>Multiple repetitions are allowed so that a single Health Condition can be attached to multiple encounters, condition-based (episode) and care-based collections.</mif:i>
                                    </mif:p>
                                 </mif:p>
                              </mif:text>
                           </mif:rationale>
