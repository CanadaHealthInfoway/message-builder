                                                                        <mif:annotations>
                                                                           <mif:definition>
                                                                              <mif:text>
                                                                                 <mif:p>
                                                                                    <mif:p>The date on which the reaction occurrence began.</mif:p>
                                                                                 </mif:p>
                                                                              </mif:text>
                                                                           </mif:definition>
                                                                           <mif:rationale>
                                                                              <mif:text>
                                                                                 <mif:p>
                                                                                    <mif:p>Indicates when evidence of the condition first appeared. May also provide information on the duration of the reaction.</mif:p>
                                                                                 </mif:p>
                                                                              </mif:text>
                                                                           </mif:rationale>
                                                                        </mif:annotations>
