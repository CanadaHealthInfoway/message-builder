                                    <mif:attribute name="classCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                                                   conformance="R"
                                                   isMandatory="true"
                                                   isStructural="true">
                                       <mif:businessName name="Request Type"/>
                                       <mif:annotations>
                                          <mif:definition>
                                             <mif:text>
                                                <mif:p>
                                                   <mif:p>
                                                      <mif:i>This distinguishes between "referral" requests and specific orders.</mif:i>
                                                   </mif:p>
                                                </mif:p>
                                             </mif:text>
                                          </mif:definition>
                                          <mif:usageNotes>
                                             <mif:text>
                                                <mif:p>
                                                   <mif:p>In the near term, the only way to request delivery of care is via a referral (PCPR). However, in the future, specific encounter scheduling messages may be defined. References to those new types of records would use ENC.</mif:p>
                                                </mif:p>
                                             </mif:text>
                                          </mif:usageNotes>
                                          <mif:rationale>
                                             <mif:text>
                                                <mif:p>
                                                   <mif:p>
                                                      <mif:i>Needed to know what query to execute to retrieve details about the request. (I.e. Get Patient Referral vs. Get Care Composition Order.)</mif:i>
                                                   </mif:p>
                                                </mif:p>
                                             </mif:text>
                                          </mif:rationale>
