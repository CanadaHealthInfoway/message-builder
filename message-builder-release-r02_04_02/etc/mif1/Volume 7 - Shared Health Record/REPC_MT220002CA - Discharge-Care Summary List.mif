               <mif:targetConnection name="responsibleParty" minimumMultiplicity="1" maximumMultiplicity="1"
                                     conformance="R"
                                     isMandatory="true"
                                     sortKey="AC________">
                  <mif:derivationSupplier staticModelDerivationId="1" className="Act" associationEndName="participation"/>
                  <mif:participantClass>
                     <mif:class name="ResponsibleParty" isAbstract="false">
                        <mif:businessName name="*b: supervised by"/>
                        <mif:derivationSupplier staticModelDerivationId="1" className="Participation"/>
                        <mif:graphicRepresentation presentation="HL7">
                           <mif:graphElement shapeId="Sheet.1" containerDiagramName="Main" shapeTemplate="Participation"
                                             textWrappingWidth="66.0"
                                             nodeOrientation="Left">
                              <mif:position x="1.891" y="2.292"/>
                              <mif:size width="3.427" height="0.436"/>
                           </mif:graphElement>
                        </mif:graphicRepresentation>
                        <mif:annotations>
                           <mif:definition>
                              <mif:text>
                                 <mif:p>
                                    <mif:p>
                                       <mif:i>This identifies the person that has primary responsibility for the actions of the author in creating the Discharge-Care Summary. The supervising provider is not necessarily present when the action is performed, but is professionally accountable for the action through the power to delegate, and the duty to review actions with the performing actor after the fact.</mif:i>
                                    </mif:p>
                                 </mif:p>
                              </mif:text>
                           </mif:definition>
                           <mif:usageNotes>
                              <mif:text>
                                 <mif:p>
                                    <mif:p>
                                       <mif:i>If not specified, it is assumed that the author is acting under their own professional authority. I.e. If the author and supervisor are the same, only the author should be in the instance.</mif:i>
                                    </mif:p>
                                 </mif:p>
                              </mif:text>
                           </mif:usageNotes>
                           <mif:rationale>
                              <mif:text>
                                 <mif:p>
                                    <mif:p>
                                       <mif:i>Needed in circumstances where one provider is acting under the professional authority of another. E.g. Pharmacy technician under pharmacist, resident under supervising physician, etc.</mif:i>
                                    </mif:p>
                                    <mif:p>
                                       <mif:i>Tracking responsibility is essential for authorization and regulatory purposes and is therefore mandatory. It also provides a secondary person to contact for follow-up.</mif:i>
                                    </mif:p>
                                 </mif:p>
                              </mif:text>
                           </mif:rationale>
                        </mif:annotations>
                        <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                                       conformance="R"
                                       isMandatory="true"
                                       fixedValue="RESP"
                                       isStructural="true">
                           <mif:derivationSupplier staticModelDerivationId="1" className="Participation" attributeName="typeCode"/>
                           <mif:type name="CS"/>
                           <mif:supplierDomainSpecification codingStrength="CNE" codeSystemName="ParticipationType" mnemonic="RESP"/>
                        </mif:attribute>
