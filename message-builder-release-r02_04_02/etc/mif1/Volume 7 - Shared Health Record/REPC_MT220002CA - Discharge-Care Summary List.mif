                                                                                                                     <mif:annotations>
                                                                                                                        <mif:definition>
                                                                                                                           <mif:text>
                                                                                                                              <mif:p>
                                                                                                                                 <mif:p>Describes one of the conditions associated with the patient as part of the encounter.</mif:p>
                                                                                                                              </mif:p>
                                                                                                                           </mif:text>
                                                                                                                        </mif:definition>
                                                                                                                        <mif:rationale>
                                                                                                                           <mif:text>
                                                                                                                              <mif:p>
                                                                                                                                 <mif:p>Commonly used for reporting, but also useful for searching and for obtaining a general understanding of the encounter.</mif:p>
                                                                                                                              </mif:p>
                                                                                                                           </mif:text>
                                                                                                                        </mif:rationale>
