                                                                  <mif:definition>
                                                                     <mif:text>
                                                                        <mif:p>
                                                                           <mif:p>Indicates the type of dispensing event that is performed. Examples include: Trial Fill, Completion of Trial, Partial Fill, Emergency Fill, Samples, etc.</mif:p>
                                                                        </mif:p>
                                                                     </mif:text>
                                                                  </mif:definition>
                                                                  <mif:rationale>
                                                                     <mif:text>
                                                                        <mif:p>
                                                                           <mif:p>Indicates reason for the size of dispense. Because it defines what type of dispense is occurring, the attribute is mandatory.</mif:p>
                                                                        </mif:p>
                                                                     </mif:text>
                                                                  </mif:rationale>
