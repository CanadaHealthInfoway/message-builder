                                                                     <mif:text>
                                                                        <mif:p>
                                                                           <mif:p>The overall amount of amount medication to be dispensed under this prescription. Includes any first fills (trials, aligning quantities), the initial standard fill plus all refills.</mif:p>
                                                                        </mif:p>
                                                                     </mif:text>
                                                                  </mif:definition>
                                                                  <mif:rationale>
                                                                     <mif:text>
                                                                        <mif:p>
                                                                           <mif:p>Sets upper limit for medication to be dispensed. Can be used to verify the intention of the prescriber with respect to the overall medication. Used for comparison when determining whether additional quantity may be dispensed in the context of a part-fill prescription.</mif:p>
                                                                        </mif:p>
                                                                        <mif:p>
                                                                           <mif:p>Narcotics must always be specified as a total quantity.</mif:p>
                                                                        </mif:p>
                                                                     </mif:text>
                                                                  </mif:rationale>
                                                               </mif:annotations>
                                                               <mif:derivationSupplier staticModelDerivationId="1" className="Supply" attributeName="quantity"/>
