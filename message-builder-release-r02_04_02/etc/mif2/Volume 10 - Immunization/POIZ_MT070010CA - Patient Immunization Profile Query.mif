   <mif:entryPoint graphicLinkId="D7D2D294-E2DB-6EC5-F8B6-824D3519B27A" id="POIZ_MT070010CA"
                   name="Patient Immunization Profile Query"
                   className="ParameterList">
      <mif:annotations>
         <mif:documentation>
            <mif:description>
               <mif:text>
                  <html:p xmlns:html="http://www.w3.org/1999/xhtml">Entry point for Patient Immunization Profile Query message. The intent of this query is to retrieve the complete immunization profile for a specific patient.</html:p>
               </mif:text>
            </mif:description>
         </mif:documentation>
      </mif:annotations>
   </mif:entryPoint>
