<?xml version="1.0" encoding="UTF-8"?>
<mif:interaction xmlns:mif="urn:hl7-org:v3/mif2" schemaVersion="2.1.3" name="010840"
                 title="Common Order Order Abort Request">
   <mif:businessName name="Abort prescription request"/>
   <mif:packageLocation root="DEFN" section="HM" subSection="PO" domain="RX" artifact="IN"
                        realmNamespace="CA"
                        id="010840"/>
   <mif:annotations>
      <mif:documentation>
         <mif:description>
            <mif:text>Requests that the specified prescription be aborted (further dispensing or administrative must cease).</mif:text>
         </mif:description>
      </mif:documentation>
   </mif:annotations>
   <mif:invokingTriggerEvent root="DEFN" section="HM" subSection="PO" domain="RX" artifact="TE"
