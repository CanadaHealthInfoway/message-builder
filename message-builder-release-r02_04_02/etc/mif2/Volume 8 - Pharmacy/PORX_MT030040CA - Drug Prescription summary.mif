                        isImmutable="false">
            <mif:businessName name="Refusal To Fill Date"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">The date that the dispenser refused to fill the prescitpion</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">May be important to down stream providers to know when the refusal occured.</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Is marked as populated as it may not always be known for historical data pre- loaded into the EHR</html:p>
                     </mif:text>
                  </mif:rationale>
