               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Indicates a non-clinical-issue based reason for refusing to fill.</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Indicates a non-clinical-issue based reason for refusing to fill. Allows CWE for non-coded reasons.</html:p>
                     </mif:text>
                  </mif:rationale>
