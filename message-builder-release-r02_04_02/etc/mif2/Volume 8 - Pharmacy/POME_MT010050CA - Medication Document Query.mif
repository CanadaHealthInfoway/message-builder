            <mif:businessName name="C:Prescribing Indication Diagnosis Code"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Returns documents which relate to a particular diagnosis</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Useful for finding protocols and drug monographs associated with an indication</html:p>
