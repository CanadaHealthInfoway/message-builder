   <mif:entryPoint graphicLinkId="14312DA5-63ED-E05F-9249-F4BCC88DFFB1" id="POME_MT010050CA"
                   name="Medication Document Query"
                   className="ParameterList">
      <mif:annotations>
         <mif:documentation>
            <mif:description>
               <mif:text>
                  <html:p xmlns:html="http://www.w3.org/1999/xhtml">Entry point for medication document query message</html:p>
