   <mif:containedClass>
      <mif:class graphicLinkId="52E24E97-3C3D-BB93-F315-54FB18523542" name="Coverage"
                 isAbstract="false">
         <mif:businessName name="Coverage Extensions"/>
         <mif:derivedFrom staticModelDerivationId="1" className="FinancialContract"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">An authorization issued by a payor to cover a drug not previously covered by a patient's drug plan.</html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Allows conveying special coverage information between providers.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
