                 isAbstract="false">
         <mif:businessName name="g:includes"/>
         <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Indicates other patient information that is an important consideration for the prescription. This information is limited to height and weight.</html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Allows patient height and weight to be conveyed to the pharmacy for dosage calculation or verification</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">The additional repetitions are to allow for capturing of additional concepts beyond height and weight without impacting the message structure should future versions of the specification allow.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="PERT"
