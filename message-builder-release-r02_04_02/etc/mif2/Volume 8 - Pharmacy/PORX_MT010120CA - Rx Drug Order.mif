   <mif:containedClass>
      <mif:class graphicLinkId="C1385F1B-D755-6AEB-A7E7-3F4D0723955D" name="Precondition"
                 isAbstract="false">
         <mif:businessName name="Non-authoritative?"/>
         <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">If true, indicates that the prescription is non-authoritative. I.e. A paper copy must be viewed before the prescription can be dispensed.</html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Allows distinguishing prescriptions where the paper copy is authoritative vs. the electronic copy being authoritative.</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">This attribute behaves as a 'mandatory' element where the presence of the association indicates that a paper prescription is needed and the absence of the association indicates that one is not.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
