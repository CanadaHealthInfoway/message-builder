<?xml version="1.0" encoding="UTF-8"?>
<mif:interaction xmlns:mif="urn:hl7-org:v3/mif2" schemaVersion="2.1.3" name="122103"
                 title="Placer Order Promise Activate Request">
   <mif:businessName name="Promise Activate"/>
   <mif:packageLocation root="DEFN" section="HM" subSection="PO" domain="LB" artifact="IN"
                        realmNamespace="CA"
                        id="122103"/>
   <mif:annotations>
      <mif:documentation>
         <mif:description>
            <mif:text>This interaction is a Promise Activate Response. This interaction is used when a filler communicates a response to a order fulfillment request. This is the fillers promise to fulfill the order. There is an expectation that once the filler issues the promise activate response, the filler will fulfill the order with a subsequent result complete interaction.</mif:text>
         </mif:description>
      </mif:documentation>
   </mif:annotations>
   <mif:invokingTriggerEvent root="DEFN" section="HM" subSection="PO" domain="LB" artifact="TE"
                             realmNamespace="CA"
                             id="002100"/>
   <mif:argumentMessage root="DEFN" section="IM" subSection="MC" domain="CI" artifact="MT"
                        realmNamespace="CA"
                        id="002100">
      <mif:parameterModel parameterName="ControlActEvent" root="DEFN" section="IM" subSection="MC"
                          domain="AI"
                          artifact="MT"
                          realmNamespace="CA"
                          id="700210"
                          traversalName="controlActEvent">
         <mif:parameterModel parameterName="Act" root="DEFN" section="HM" subSection="PO" domain="LB"
                             artifact="MT"
                             realmNamespace="CA"
                             id="002000"
                             traversalName="promiseChoice">
            <mif:specialization className="PromiseGroup" traversalName="promiseGroup"/>
            <mif:specialization className="BatteryPromise" traversalName="batteryPromise"/>
            <mif:specialization className="ObservationPromise" traversalName="observationPromise"/>
         </mif:parameterModel>
      </mif:parameterModel>
   </mif:argumentMessage>
</mif:interaction>