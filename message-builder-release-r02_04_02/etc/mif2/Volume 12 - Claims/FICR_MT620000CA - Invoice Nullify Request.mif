   <mif:containedClass>
      <mif:class graphicLinkId="90D068DE-7AAC-A1B1-00F8-DC3B567A6A94" name="InvoiceElementGroup"
                 isAbstract="false">
         <mif:businessName name="Invoice Type"/>
         <mif:derivedFrom staticModelDerivationId="1" className="InvoiceElement"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition graphicLinkId="5411A8E5-6BF8-3E9F-EB3A-297395F36D47">
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Cancel reasons are noted in the control wrapper</html:p>
                  </mif:text>
               </mif:definition>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="classCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="INVE"
                        isImmutable="true">
