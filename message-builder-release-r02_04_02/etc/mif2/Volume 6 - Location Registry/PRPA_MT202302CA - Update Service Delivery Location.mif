   </mif:association>
   <mif:association graphicLinkId="45105367-7666-ADF8-C13B-57F39013FA74" sortKey="10">
      <mif:traversableConnection name="asIdentifiedLocation" minimumMultiplicity="1" maximumMultiplicity="5"
                                 conformance="R"
                                 isMandatory="true"
