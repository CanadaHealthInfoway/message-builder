   <mif:ownedEntryPoint id="REPC_MT420001CA" name="Coded Observation Record" isAbstract="false">
      <mif:graphicRepresentation>
         <mif:graphElement shapeId="Sheet.41" containerDiagramName="Main">
            <mif:position x="9.462" y="0.028"/>
            <mif:size width="1.881" height="0.453"/>
