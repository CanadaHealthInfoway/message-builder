                                                                                             <mif:text>
                                                                                                <mif:p>
                                                                                                   <mif:p>Represents the rendered text content for the section.</mif:p>
                                                                                                </mif:p>
                                                                                             </mif:text>
                                                                                          </mif:definition>
                                                                                          <mif:rationale>
