                                                                           <mif:definition>
                                                                              <mif:text>
                                                                                 <mif:p>
                                                                                    <mif:p>The covered party(s) of the insurance policy of whom the product or service occurs, but is not necessarily present in the service. The party(s) that derives benefits from the act.</mif:p>
                                                                                 </mif:p>
                                                                              </mif:text>
                                                                           </mif:definition>
                                                                           <mif:rationale>
                                                                              <mif:text>
                                                                                 <mif:p>
                                                                                    <mif:p>On an invoice, there must be a Covered Party specified against an insurance policy.</mif:p>
                                                                                 </mif:p>
                                                                              </mif:text>
                                                                           </mif:rationale>
                                                                        </mif:annotations>
