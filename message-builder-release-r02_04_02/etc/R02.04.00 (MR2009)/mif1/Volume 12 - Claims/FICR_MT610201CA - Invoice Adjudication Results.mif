                                                               <mif:definition>
                                                                  <mif:text>
                                                                     <mif:p>
                                                                        <mif:p>Policy or Account of covered party name in invoice.</mif:p>
                                                                     </mif:p>
                                                                  </mif:text>
                                                               </mif:definition>
                                                            </mif:annotations>
                                                            <mif:attribute name="classCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
