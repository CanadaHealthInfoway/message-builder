   <mif:triggerEvent name="100001"
                     title="Polling Message Transmission Event Send Poll Transmission Request"
                     invokingInteractionId="MCCI_IN100001CA">
      <mif:businessName name="Send Poll Message 1 Request"/>
      <mif:packageLocation root="DEFN" section="IM" subSection="MC" domain="CI" artifact="TE" realm="CA"
                           id="100001"/>
      <mif:annotations>
         <mif:description>
            <mif:text>The processing of a poll operation to retrieve an interaction from a remotely managed interaction queue has started.</mif:text>
         </mif:description>
      </mif:annotations>
   </mif:triggerEvent>
   <mif:triggerEvent name="102002" title="Polling Message Transmission Poll Status Query"
