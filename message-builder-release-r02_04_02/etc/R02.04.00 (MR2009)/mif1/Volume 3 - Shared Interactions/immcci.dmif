   <mif:hl7Interaction name="100004" title="Polling Message Transmission Event Request">
      <mif:businessName name="Accept Ack Poll for Next Message"/>
      <mif:packageLocation root="DEFN" section="IM" subSection="MC" domain="CI" artifact="IN" realm="CA"
                           id="100004"/>
      <mif:annotations>
         <mif:description>
            <mif:text>Communication level acknowledgement that polled message received and immediately making a poll request for the next message in the queue. Note: This interaction is invoked, where appropriate, as a receiver responsibility.</mif:text>
         </mif:description>
      </mif:annotations>
      <mif:callAction root="DEFN" section="IM" subSection="MC" domain="CI" artifact="TE" realm="CA"
                      version="01"
                      id="100003"/>
      <mif:parameterTypeModel root="DEFN" section="IM" subSection="MC" domain="CI" artifact="MT-deprecated"
                              realm="CA"
                              id="002200"/>
   </mif:hl7Interaction>
