                           id="100001"/>
      <mif:annotations>
         <mif:description>
            <mif:text>Sends HL7 poll request control message to an HL7 message queue manager.</mif:text>
         </mif:description>
      </mif:annotations>
      <mif:callAction root="DEFN" section="IM" subSection="MC" domain="CI" artifact="TE" realm="CA"
                      id="100001"/>
      <mif:parameterTypeModel root="DEFN" section="IM" subSection="MC" domain="CI" artifact="MT-deprecated"
                              realm="CA"
                              id="102001"/>
      <mif:receiverResponsibility triggerEventName="Send Poll Message Error"
                                  interactionName="Polling Message Transmission Event"/>
   </mif:hl7Interaction>
   <mif:hl7Interaction name="100005" title="Polling Message Transmission Event">
      <mif:businessName name="Exception Ack in Response to Poll Request"/>
      <mif:packageLocation root="DEFN" section="IM" subSection="MC" domain="CI" artifact="IN" realm="CA"
                           id="100005"/>
      <mif:annotations>
         <mif:description>
            <mif:text>Communication level acknowledgement indicating exception as to why message payload not returned by poll request that has been received. Note: This interaction is invoked, where appropriate, as a receiver responsibility.</mif:text>
         </mif:description>
      </mif:annotations>
      <mif:callAction root="DEFN" section="IM" subSection="MC" domain="CI" artifact="TE" realm="CA"
