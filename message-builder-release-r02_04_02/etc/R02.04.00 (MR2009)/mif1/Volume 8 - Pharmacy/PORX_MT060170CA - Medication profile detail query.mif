                           <mif:businessName name="Include Event History Indicator"/>
                           <mif:annotations>
                              <mif:definition>
                                 <mif:text>
                                    <mif:p>
                                       <mif:p>Indicates whether or not history events associated with a prescription order, prescription dispense and/or active medications are to be returned along with the detailed information.</mif:p>
                                    </mif:p>
                                    <mif:p>
                                       <mif:p>"Pending" changes will be returned regardless of the setting of this flag.</mif:p>
                                    </mif:p>
                                 </mif:text>
