         <mif:businessName name="Invoice Type"/>
         <mif:derivedFrom staticModelDerivationId="1" className="InvoiceElement"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition graphicLinkId="F749BCE7-4543-2616-2C8F-D864354AAF27">
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Cancel reasons are noted in the control wrapper</html:p>
