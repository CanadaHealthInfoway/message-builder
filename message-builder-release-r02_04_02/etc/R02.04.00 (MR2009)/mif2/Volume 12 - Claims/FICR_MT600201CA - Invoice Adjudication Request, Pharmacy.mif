   <mif:containedClass>
      <mif:class graphicLinkId="FF0801BE-F542-5E64-4501-E4F6D63BFEB4" name="PaymentRequest"
                 isAbstract="false">
         <mif:businessName name="Invoice Identifier"/>
         <mif:derivedFrom staticModelDerivationId="1" className="FinancialTransaction"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Set of Identifiers that uniquely identify the Invoice. Could reference an Invoice Number with associated indentifers</html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Details of the claim for which payment is being requested.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="classCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="XACT"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="FinancialTransaction"
                             attributeName="classCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ActClass" code="XACT"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="moodCode" sortKey="2" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="RQO"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="FinancialTransaction"
                             attributeName="moodCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ActMood" code="RQO"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="id" sortKey="3" minimumMultiplicity="0" maximumMultiplicity="5"
                        conformance="R"
                        isMandatory="false"
                        isImmutable="false">
            <mif:businessName name="Invoice Identifier"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Set of Identifiers that uniquely identify the Invoice. Could reference an Invoice Number with associated indentifers</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Invoice Identifier: Used for management of payment request objects by adjudicators to help with traceability of messages to their applications. This does not need to be surfaced to the provider. Can also be used to detect duplicate invoices.</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">There are some situations where more than 1 identifier for this object can be included in a message.</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">For example:</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">1. unique invoice identifier, independent of adjudicator recipient.</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">2. sequential invoice identifier by adjudicator.</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Obligation on adjudicator is to return and communicate about this item with all identifiers (i.e. identifier 1. and 2.). At present, there are no messages where this occurs (Jun 04)</html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="FinancialTransaction" attributeName="id"/>
            <mif:type name="SET">
               <mif:argumentDatatype name="II.PUBLIC"/>
            </mif:type>
         </mif:attribute>
         <mif:attribute name="amt" sortKey="4" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        isImmutable="false">
            <mif:businessName name="Total Billed Amount"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text/>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">OID (object identifier) + unique number generated by the pharmacy software for the complete invoice.</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Must not be the same identifier as the InvoiceElementGroup.id.</html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="FinancialTransaction"
                             attributeName="amt"/>
            <mif:type name="MO.CAD"/>
         </mif:attribute>
      </mif:class>
   </mif:containedClass>
   <mif:containedClass>
      <mif:class graphicLinkId="89EBB7BF-857A-C7C4-191B-3EFD4609A7C8"
                 name="PaymentRequestAttention"
                 isAbstract="false">
         <mif:businessName name="Attention to contacts"/>
         <mif:derivedFrom staticModelDerivationId="1" className="Participation"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Invoice Recipient Contact person</html:p>
                  </mif:text>
               </mif:definition>
               <mif:usageNotes>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">If specified for the Payor contact, will always result in manual review by said individual - use with caution.</html:p>
                  </mif:text>
               </mif:usageNotes>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">For Pharmacy dispense Invoices (RXDINV, RXCINV), this is NOT permitted to facilitate real-time processing.</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">For Pharmacy clinical product (CPINV) and clinical services (CSINV) Invoices, this is permitted to indicate who the Invoice is directed to with the Adjudicator, likely for manual review.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="PPRF"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="Participation" attributeName="typeCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ParticipationType" code="PPRF"/>
            </mif:vocabulary>
         </mif:attribute>
      </mif:class>
   </mif:containedClass>
   <mif:containedClass>
      <mif:class graphicLinkId="4144AD3F-C1C5-9569-0CCF-E13737844231" name="PaymentRequestPayee"
