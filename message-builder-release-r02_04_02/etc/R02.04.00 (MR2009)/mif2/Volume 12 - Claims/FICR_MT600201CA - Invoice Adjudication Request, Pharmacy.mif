      <mif:class graphicLinkId="68BA6B71-20FC-95C7-33D5-15618046CBED"
                 name="CoveredPartyAsPatient"
                 isAbstract="false">
         <mif:businessName name="Covered Party Information"/>
         <mif:derivedFrom staticModelDerivationId="1" className="Role"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition graphicLinkId="1890CEFE-2DAC-CB14-A4B8-E12A94660EB0">
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Covered Person Identifier</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">(Assigned by carrier - root of OID)</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">= Subscriber+Dependents</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Can be a single covered party,</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">an organization, an animal</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">or a group of patients</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">and/or a group of animals.</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Covered party may be a patient who for the purposes of the invoice is a covered party.</html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">For Adjudication Results: Policy information can be returned in an EOB in adjudication results to inform a Provider of new policy information for a policy that was not submitted in the Invoice by the Provider (to be confirmed and specified in the NeCST Message Specifications).</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="classCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="COVPTY"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="Role" attributeName="classCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="RoleClass" code="COVPTY"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="id" sortKey="2" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="false"
                        isImmutable="false">
            <mif:businessName name="Covered Party Identifier"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">The insurance policy identifier of the party covered.</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:usageNotes>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">For Invoice, Pre-Determination and Coverage Extension requests: If this information is not supplied, the Adjudicator may attempt to determine the information from other patient information supplied in the message (e.g. name, date of birth, gender) and would return this information with the response. If this cannot be obtained in a real time environment, the message may be rejected, depending on the Application Role (if Application Role = Final).</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">For Invoice, Pre-Determination and Coverage Extension results: This information is mandatory.</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">RxS1: This information is mandatory. Scheme to be confirmed.</html:p>
                     </mif:text>
                  </mif:usageNotes>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">In the absence of a Covered Party identifier, additional information to identify the individual may be required (e.g. name, date of birth).</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">For Invoice: In good faith Invoices, the Policy Identifier, Covered Party Identifier and Policy Holder Identifier is not present, and therefore this attribute cannot be made mandatory.</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">For new borns &amp; children not registered to date such as adopted children (i.e. without a covered party identifier), the parents' policy is specified (in PolicyOrAccount.id). The new born/child is noted as the covered party.</html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Role" attributeName="id"/>
            <mif:type name="II.BUS"/>
         </mif:attribute>
         <mif:attribute name="code" sortKey="3" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        isImmutable="false">
            <mif:businessName name="Covered Party Relationship to Policy Holder"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">The covered party relationship to the Policy Holder.</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:usageNotes>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">For Invoice and Pre-Determination Request &amp; all Results: Mandatory, otherwise Required.</html:p>
                     </mif:text>
                  </mif:usageNotes>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Reason for being covered, including spouse, child, students, handicapped</html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Role" attributeName="code"/>
            <mif:type name="CV"/>
            <mif:vocabulary>
               <mif:conceptDomain name="CoverageRoleType"/>
            </mif:vocabulary>
         </mif:attribute>
      </mif:class>
   </mif:containedClass>
