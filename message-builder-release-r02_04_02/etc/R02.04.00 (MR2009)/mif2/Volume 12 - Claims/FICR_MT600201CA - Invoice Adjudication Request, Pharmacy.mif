   <mif:containedClass>
      <mif:class graphicLinkId="5836654A-C3EE-7B62-8C1A-711A8F42630C"
                 name="PaymentRequestReason"
                 isAbstract="false">
         <mif:businessName name="Payment Request for"/>
         <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">The reason or rationale for the invoice.</html:p>
                  </mif:text>
               </mif:definition>
               <mif:usageNotes>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">For Pharmacy dispense Invoices (RXDINV, RXCINV), only 1 Invoice Grouping is permitted to facilitate real-time processing.</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">For Pharmacy clinical product (CPINV) and clinical services (CSINV) Invoices, 1 or more Invoice Groupings are permitted.</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">For Coverage Extension Requests, only 1 Invoice Grouping per Payment Request.</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">RxS1: Only allow 1 Invoice Grouping. Maximum cardinality is 1.</html:p>
                  </mif:text>
               </mif:usageNotes>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">For Cancel Request: To cancel an Invoice with more than 1 Invoice Grouping, each individual Invoice Group must be cancelled separately using an individual Cancel Invoice Request message.</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">For Cancel Results: Must include a Payment Request and Invoice Element Group.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="RSON"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"
                             attributeName="typeCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ActRelationshipType" code="RSON"/>
            </mif:vocabulary>
         </mif:attribute>
      </mif:class>
   </mif:containedClass>
