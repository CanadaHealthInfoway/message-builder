                        maximumLength="20"
                        isImmutable="false">
            <mif:businessName name="E:Drug Lot Number"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Identification of a batch in which a specific manufactured drug belongs.</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Useful in tracking for recalls but may not always exist.</html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
