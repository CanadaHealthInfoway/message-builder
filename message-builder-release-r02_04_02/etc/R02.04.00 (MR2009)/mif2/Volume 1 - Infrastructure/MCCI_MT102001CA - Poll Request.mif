   <mif:containedClass>
      <mif:class graphicLinkId="8597ECD6-2090-4300-6017-1916094DE4B2" name="RespondTo"
                 isAbstract="false">
         <mif:businessName name="to be responded to by"/>
         <mif:derivedFrom staticModelDerivationId="1" className="CommunicationFunction"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Indicates the interaction that should receive the response to this interaction. Used when different from the sender of the original interaction.</html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">In complex routing environments, the acknowledgements may need to be sent to a system other than the one which constructed the original message. This association is optional because not all environments will support responses to systems other than the original sender.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="RSP"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="CommunicationFunction"
                             attributeName="typeCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="CommunicationFunctionType" code="RSP"/>
            </mif:vocabulary>
