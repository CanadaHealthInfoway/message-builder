                        isMandatory="false"
                        isImmutable="false">
            <mif:businessName name="KB:Respond to Network Address"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Indicates the address to send acknowledgments of this message to.</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Needed when the address to respond to is different than that of the sender. This is optional because not all environments require network addresses. It is required when communicating using SOAP.</html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
               <mif:appInfo>
                  <mif:mapping sourceName="SOAP">
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">soap:Header\wsa:ReplyTo</html:p>
                     </mif:text>
                  </mif:mapping>
               </mif:appInfo>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="CommunicationFunction"
                             attributeName="telecom"/>
            <mif:type name="TEL.URI"/>
         </mif:attribute>
      </mif:class>
