                        isImmutable="false">
            <mif:businessName name="JK:Receiver Organization Identifier"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Name of receiver application.</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
