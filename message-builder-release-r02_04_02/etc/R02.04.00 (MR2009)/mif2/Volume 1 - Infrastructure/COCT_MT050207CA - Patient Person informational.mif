            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Telephone, fax and/or e-mail addresses intended as the principal means of contact for the patient.</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:usageNotes>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Contact information specific to the patient for a particular action (prescription, lab test, etc.) will be conveyed as part of the payload.</html:p>
                     </mif:text>
                  </mif:usageNotes>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Provides basic contact information for the patient.</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Important for following up with patient but not always available, and therefore only marked as 'populated'.</html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
               <mif:appInfo>
