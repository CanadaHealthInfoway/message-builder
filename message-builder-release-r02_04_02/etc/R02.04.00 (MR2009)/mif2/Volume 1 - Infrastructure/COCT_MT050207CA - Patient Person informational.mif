                        isImmutable="false">
            <mif:businessName name="B:Patient Name"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">The name by which the patient is known.</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Used by to help identify the patient and also when referring to the patient</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">The element is populated because the patient's name is important for display. However, there may be circumstances where a name is not known by the reporting system.</html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
               <mif:appInfo>
