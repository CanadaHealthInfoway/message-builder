         <mif:businessName name="Patient"/>
         <mif:derivedFrom staticModelDerivationId="1" className="Patient"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">A person who is receiving or may receive healthcare services and has personal attributes (e.g. name, birth date).</html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Information used to identify the patient and to support clinical decisions. This is used when the patient is not known to the client registry but information still needs to be communicated. E.g. lab or drug reporting to PHS for an unconscious or otherwise unidentifiable patient.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="classCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
