      <mif:class graphicLinkId="EABCEDA6-8557-7DF9-0B33-0C195ABB7542" name="ResponsibleParty"
                 isAbstract="false">
         <mif:businessName name="*b: supervised by"/>
         <mif:derivedFrom staticModelDerivationId="1" className="Participation"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                        <html:i>This identifies the person that has primary responsibility for the actions of the author in creating the Referral. The supervising provider is not necessarily present when the action is performed, but is professionally accountable for the action through the power to delegate, and the duty to review actions with the performing actor after the fact.</html:i>
                     </html:p>
                  </mif:text>
               </mif:definition>
               <mif:usageNotes>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                        <html:i>If not specified, it is assumed that the author is acting under their own professional authority. I.e. If the author and supervisor are the same, only the author should be in the instance.</html:i>
                     </html:p>
                  </mif:text>
               </mif:usageNotes>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                        <html:i>Needed in circumstances where one provider is acting under the professional authority of another. E.g. Pharmacy technician under pharmacist, resident under supervising physician, etc.</html:i>
                     </html:p>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                        <html:i>Tracking responsibility is essential for authorization and regulatory purposes and is therefore mandatory. It also provides a secondary person to contact for follow-up.</html:i>
                     </html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="RESP"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="Participation" attributeName="typeCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ParticipationType" code="RESP"/>
