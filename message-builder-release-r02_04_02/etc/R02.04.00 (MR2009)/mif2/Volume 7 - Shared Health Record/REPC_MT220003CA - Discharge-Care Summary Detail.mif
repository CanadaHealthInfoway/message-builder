                        isImmutable="false">
            <mif:businessName name="M: Document Overview Content"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Provides the primary rendered textual content of the document.</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">E.g. The cover letter for a referral, the overview portion of a report, etc.</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:usageNotes>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">There is no guidance provided within the standard on how report content should be organized or formatted. Guidance may be provided by professional colleges, specialty groups or others.</html:p>
                     </mif:text>
                  </mif:usageNotes>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Represents the principle content of the document and therefore is mandatory.</html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="text"/>
