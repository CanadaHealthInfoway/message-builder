      <mif:specializedClass>
         <mif:class name="MedicalCondition" isAbstract="false">
            <mif:businessName name="Medical Condition"/>
            <mif:derivationSupplier staticModelDerivationId="1" className="Observation"/>
            <mif:graphicRepresentation presentation="HL7">
               <mif:graphElement shapeId="Sheet.1" containerDiagramName="Main" shapeTemplate="Act"
                                 isWidthAutoSize="true"
                                 isHeightAutoSize="false">
                  <mif:position x="2.249" y="0.967"/>
                  <mif:size width="3.202" height="2.931"/>
               </mif:graphElement>
            </mif:graphicRepresentation>
            <mif:annotations>
               <mif:definition>
                  <mif:text>
                     <mif:p>
                        <mif:p>A record of a patient's medical condition. Includes diseases, disabilities, pregnancy, lactation and other clinical conditions of interest.</mif:p>
                     </mif:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <mif:p>
                        <mif:p>Necessary component of a person's overall medication and clinical profile. Helps with contraindication checking.</mif:p>
                     </mif:p>
                  </mif:text>
               </mif:rationale>
               <mif:constraint>
                  <mif:text>
                     <mif:p>
                        <mif:p>Value is mandatory if not using SNOMED</mif:p>
                     </mif:p>
                  </mif:text>
                  <mif:graphicRepresentation>
                     <mif:graphElement shapeId="Sheet.211" containerDiagramName="Main" isWidthAutoSize="true"
                                       isHeightAutoSize="true">
                        <mif:position x="0.069" y="2.276"/>
                        <mif:size width="1.164" height="0.444"/>
                        <mif:anchorage connectToShapeId="Sheet.211">
                           <mif:position x="0.651" y="2.499"/>
                           <mif:graphEdge>
                              <mif:anchor connectToShapeId="Sheet.1">
                                 <mif:position x="2.249" y="2.433"/>
                              </mif:anchor>
                           </mif:graphEdge>
                        </mif:anchorage>
                     </mif:graphElement>
                  </mif:graphicRepresentation>
               </mif:constraint>
               <mif:constraint>
                  <mif:text>
                     <mif:p>
                        <mif:p>Code is fixed to DX if not using SNOMED</mif:p>
                     </mif:p>
                  </mif:text>
