               </mif:graphElement>
            </mif:graphicRepresentation>
            <mif:annotations>
               <mif:definition>
                  <mif:text>
                     <mif:p>
                        <mif:p>A record of a patient's medical condition. Includes diseases, disabilities, pregnancy, lactation and other clinical conditions of interest.</mif:p>
                     </mif:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <mif:p>
                        <mif:p>Necessary component of a person's overall medication and clinical profile. Helps with contraindication checking.</mif:p>
                     </mif:p>
                  </mif:text>
               </mif:rationale>
               <mif:constraint>
                  <mif:text>
                     <mif:p>
                        <mif:p>Value is mandatory if not using SNOMED</mif:p>
                     </mif:p>
                  </mif:text>
                  <mif:graphicRepresentation>
                     <mif:graphElement shapeId="Sheet.118" containerDiagramName="Main" isWidthAutoSize="true"
                                       isHeightAutoSize="true">
                        <mif:position x="2.386" y="2.376"/>
                        <mif:size width="1.164" height="0.444"/>
                        <mif:anchorage connectToShapeId="Sheet.118">
                           <mif:position x="2.968" y="2.598"/>
                           <mif:graphEdge>
                              <mif:anchor connectToShapeId="Sheet.1">
                                 <mif:position x="4.535" y="2.658"/>
                              </mif:anchor>
                           </mif:graphEdge>
                        </mif:anchorage>
                     </mif:graphElement>
                  </mif:graphicRepresentation>
               </mif:constraint>
               <mif:constraint>
                  <mif:text>
                     <mif:p>
                        <mif:p>Code is fixed to DX if not using SNOMED</mif:p>
