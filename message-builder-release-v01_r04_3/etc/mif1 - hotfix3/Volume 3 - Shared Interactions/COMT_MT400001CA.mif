               <mif:annotations>
                  <mif:definition>
                     <mif:text>
                        <mif:p>
                           <mif:p>Indicates a detailed type of record to be masked. E.g. All lab tests of a given type.</mif:p>
                        </mif:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <mif:p>
                           <mif:p>Allows automatic masking of a particular type of record rather than requiring each occurrence to be masked individually.</mif:p>
                        </mif:p>
                     </mif:text>
                  </mif:rationale>
