                                       conformance="R"
                                       isMandatory="true"
                                       isStructural="false">
                           <mif:businessName name="Drug Names"/>
                           <mif:annotations>
                              <mif:definition>
                                 <mif:text>
                                    <mif:p>
                                       <mif:p>The name assigned to a drug.</mif:p>
                                    </mif:p>
                                 </mif:text>
                              </mif:definition>
                              <mif:rationale>
                                 <mif:text>
                                    <mif:p>
                                       <mif:p>Names are used for human reference communication, to allow selection from dropdowns and for local searching. Up to two names are supported: a 'search name' which is fully formed with no abbreviations, and a 'display name' which may contain abbreviations to fit within a smaller area on the screen.</mif:p>
                                    </mif:p>
                                    <mif:p>
                                       <mif:p>This attribute is marked as 'mandatory' as drugs at any level of the drug hierarchy is supposed to have a name.</mif:p>
                                    </mif:p>
                                 </mif:text>
                              </mif:rationale>
                           </mif:annotations>
                           <mif:derivationSupplier staticModelDerivationId="1" className="ManufacturedMaterial"
                                                   attributeName="name"/>
